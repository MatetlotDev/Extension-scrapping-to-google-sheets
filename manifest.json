{
    "manifest_version": 3,
    "name": "Web Scraper to Google Sheets",
    "version": "1.0",
    "description": "Scrapes data from the current page and writes it to Google Sheets.",
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "identity"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "oauth2": {
      "client_id": "CLIENT_ID",
      "scopes": [
        "https://www.googleapis.com/auth/spreadsheets"
      ]
    }
  }
  